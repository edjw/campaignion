{
  "name": "campaignion_vue",
  "version": "1.2.9",
  "description": "A bundle of libraries shared by the Vue apps in Campaignion.",
  "main": "src/index.js",
  "scripts": {
    "build": "npm run build-js && npm run build-min-js && node extract-locale.js",
    "build-js": "webpack --env dev",
    "build-min-js": "webpack --env build",
