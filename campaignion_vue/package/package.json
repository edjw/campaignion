    "axios": "0.16.2",
    "element-ui": "1.3.7",
    "es6-promise": "4.1.0",
    "vue": "2.3.4",
    "vuedraggable": "2.14.1",
    "vuex": "2.3.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-component": "0.10.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "3.5.0",
    "css-loader": "0.28.4",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "extract-loader": "0.1.0",
    "file-loader": "0.11.2",
    "mocha": "3.3.0",
    "mock-browser": "0.92.14",
    "require-dir": "0.3.2",
    "url-loader": "0.5.9",
    "vue-loader": "12.2.1",
